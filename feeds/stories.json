---
permalink: /stories.json
---
{% assign stories = site.data.stories | sort: "uploaded" | reverse %}
{
  "version": "http://jsonfeed.org/version/1",
  "title": "@muan's stories",
  "icon": "http://github.com/muan.png",
  "home_page_url": "https://muan.co/stories.json",
  "description": "Stories.",
  "items": [{% for story in stories limit: 10 %}
    {
      "id": "{{ story.id }}",
      "summary": "{{ story.meta.caption }}",
      "image": "https://photos.muan.dev/cdn-cgi/imagedelivery/-wp_VgtWlgmh1JURQ8t1mg/{{ story.id }}/public",
      "date_published": "{{ story.uploaded }}",
      "tags": "{{ story.meta.tags }}"
    }{% if stories.last != story %},{% endif %}{% endfor %}
  ]
}
